# Environment
NODE_ENV=development
PORT=3000

# Database
DB_TYPE=sqljs
DB_LOCATION=database.sqljs
DB_SYNCHRONIZE=true
DB_LOGGING=false

# JWT Configuration (Required)
JWT_SECRET=development-super-secret-jwt-key-must-be-at-least-32-characters-long-for-security
JWT_EXPIRES_IN=24h

# Logging
LOG_LEVEL=debug

# Security
CORS_ORIGIN=*
THROTTLE_TTL=60000
THROTTLE_LIMIT=100

# File Upload
UPLOAD_DEST=./uploads
MAX_FILE_SIZE=5242880

# i18n
DEFAULT_LANGUAGE=zh
FALLBACK_LANGUAGE=en

# Application
APP_NAME=Faker API
APP_DESCRIPTION=用于测试的API接口
APP_VERSION=1.0.0

# Authentication Control
AUTH_ENABLED=false
