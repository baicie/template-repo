{
    "name": "simple-node-ts",
    "version": "1.0.0",
    "description": "一个简单的 Node.js + TypeScript 项目",
    "main": "dist/index.js",
    "scripts": {
      "build": "rolldown --config rolldown.config.ts",
      "build:tsc": "tsc",
      "start": "node dist/index.js",
      "dev": "tsx src/index.ts",
      "watch": "tsx watch src/index.ts",
      "clean": "rimraf dist"
    },
    "keywords": [
      "node",
      "typescript",
      "simple"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
      "@rolldown/node": "^0.0.5",
      "@types/node": "^20.0.0",
      "rimraf": "^5.0.0",
      "rolldown": "^1.0.0-beta.42",
      "tsx": "^4.0.0",
      "typescript": "^5.0.0"
    }
  }
  